# селери бит нужно два момента.
# сам апп
python -m celery -A app worker
# и scheduler
celery -A app  beat -l info --scheduler django_celery_beat.schedulers:DatabaseScheduler

# запуск
cd site && uwsgi app/uwsgi.ini
# загрузка
python -m scripts.load


python -m celery -A app worker -B

uwsgi --ini app/uwsgi.ini --detach --attach-daemon2 'cmd=celery -A app worker --concurrency=1 &> logs/celery_beats.logs'
